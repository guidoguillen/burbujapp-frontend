{
  "name": "burbuja-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "mock-api": "json-server --watch db.json --port 3001 --host 0.0.0.0",
    "mock-api-fast": "json-server --watch db.json --port 3001 --host 0.0.0.0",
    "dev": "powershell -ExecutionPolicy Bypass -File scripts/start-dev.ps1",
    "dev:fast": "powershell -ExecutionPolicy Bypass -File scripts/start-dev.ps1 -Fast",
    "dev:android": "powershell -ExecutionPolicy Bypass -File scripts/start-dev.ps1 -AndroidOnly",
    "dev:stop": "powershell -ExecutionPolicy Bypass -File scripts/stop-dev.ps1",
    "dev:stop-force": "powershell -ExecutionPolicy Bypass -File scripts/stop-dev.ps1 -Force",
    "setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-dev-environment.ps1",
    "setup:extensions": "powershell -ExecutionPolicy Bypass -File scripts/setup-dev-environment.ps1 -InstallExtensions",
    "clone-setup": "powershell -ExecutionPolicy Bypass -File scripts/clone-and-setup.ps1"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~53.0.20",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-notifications": "^0.31.4",
    "expo-sharing": "^13.1.5",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1",
    "react-native-view-shot": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.0.0",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
